{
  "prettier.disableLanguages": ["html", "js"],
  "files.associations": {
    "js/thirdparty/*.js": "plaintext",
    "js/popper.js": "plaintext",
    "js/bootstrap.4.3.1.js": "plaintext"
  },
  "css.remoteStyleSheets": ["https://pw.sabio.la/dist/all-site.css"],
  "editor.formatOnSave": true,
  "liveServer.settings.host": "localhost",
  "liveServer.settings.port": 1932,
  "workbench.editor.highlightModifiedTabs": true,
  "files.exclude": {
    ".solutions": false,
    "dist": true,
    "node_modules": true,

    "package-lock.json": true,

    "webfonts": true
  },
  "liveServer.settings.multiRootWorkspaceName": "sabio",
  "liveServer.settings.CustomBrowser": "chrome",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ab307e",
    "activityBar.activeBorder": "#25320e",
    "activityBar.background": "#ab307e",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#25320e",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#832561",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#ab307e",
    "titleBar.activeBackground": "#832561",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#83256199",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#832561"
}
